<section class="theme-list create-web" ng-controller="registerController" ng-init="initRegisterController()">
    <div class="container">
        <h1 class="text-uppercase clearfix title">đăng ký tạo mẫu Website miễn phí</h1>
        <div class="steps clearfix">
            <div class="col-md-6 col-md-offset-3 col-sm-6 col-sm-offset-3 ">
                <ul class="clearfix">
                    <li ng-class="{ 'active' : Step==1 }">
                        <a> <span class="step" >1</span> <span class="title" style=" color: #2990ea;">Thông tin website</span> </a>
                    </li>
                    <li ng-class="{ 'active' : Step==2 }">
                        <a> <span class="step" style="background-color: #2990ea;border-color: #2990ea;">2</span> <span class="title">Chọn giao diện</span> </a>
                    </li>
                    <li ng-class="{ 'active' : Step==3 }">
                        <a> <span class="step">3</span> <span class="title">Xác nhận</span> </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="step-block-box clearfix" ng-show="Step==1">
            <div class="step-block2 clearfix">
                <h2>Bước 2: Chọn giao diện</h2>
                <ul class="nav nav-tabs">
                    <li role="presentation" class="" ng-repeat="item in FieldParents" ng-class="{'active':item.Id==FieldParentId}">
                        <a href="javascript:void(0)" ng-click="changeParents(item.Id)">{{item.Name}}</a>
                    </li>
                </ul>
                <div class="filed-menu clearfix text-center">
                    <a data-filter="" class="item" ng-repeat="item in FieldChilds" ng-class="{'active':item.Id==FieldChildId}" ng-click="changeChilds(item.Id)">{{item.Name}}</a>
                </div>
                <div class="product-template clearfix">
                    <div class="col-md-4 col-sm-4 col-sm-12" ng-repeat="item in Templates">
                        <div class=" product-item wow">
                            <div class="product-img"><a href="kho-giao-dien/%7b%7bitem.Code%7d%7d.html"><img ng-src="{{item.Image}}" class="img-responsive" alt="{{item.Name}}"></a></div>
                            <span class="circle-new" ng-if="item.IsNew==true">mới</span>
                            <div class="product-info">
                                <h2 class="name"><a href="kho-giao-dien/%7b%7bitem.Code%7d%7d.html">{{item.Name}}</a></h2>
                                <div class="price">{{item.Price}} đ</div>
                            </div>
                            <div class="overlay">
                                <div class="color-group">
                                    <ul class="color">
                                        <li ng-class="{'selected':TemplateColorId==0&&TemplateId==item.Id}" ng-click="changeColor(0,item.Id)"><a title="{{item.Name}}" style="background-color:{{item.HexColor}}"></a></li>
                                        <li ng-class="{'selected':it.Id==TemplateColorId&&TemplateId==item.Id}" ng-click="changeColor(it.Id,item.Id)" ng-repeat="it in item.ModelTpl_TemplateColor"><a title="{{it.Name}}" style="background-color:{{it.HexColor}}"></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="check" ng-if="item.Id==TemplateId"></div>
                        </div>
                    </div>
                </div>
                <nav class="text-center">
                    <paging class="small"
                            page="currentPage"
                            page-size="pageSize"
                            total="total"
                            adjacent="{{adjacent}}"
                            dots="{{dots}}"
                            scroll-top="{{scrollTop}}"
                            hide-if-empty="{{hideIfEmpty}}"
                            ul-class="{{ulClass}}"
                            active-class="{{activeClass}}"
                            disabled-class="{{disabledClass}}"
                            show-prev-next="{{showPrevNext}}"
                            paging-action="DoCtrlPagingAct('Paging Clicked', page, pageSize, total)">
                    </paging>
                </nav>
                <div class="form-group button-group">
                    
                    <div class="button pull-right">
                        <a class="btn btn-warning" href="javascript:void(0)" ng-click="backStep1()"><i class="fa fa-mail-reply"></i> Quay lại</a>
                        <button class="btn btn-default" type="button" ng-click="step3()">Tiếp tục</button>
                    </div>
                </div>
